  menssages:
  - message: Estamos jodidos.
    npcName: Oliver
    slowText: 0
    thinking: 0
    methodName: 
    options: []
  - message: "\xBFT\xFA crees?"
    npcName: Juno
    slowText: 0
    thinking: 0
    methodName: 
    options: []
  - message: "S\xED. Si entre hoy y ma\xF1ana sale otro problema m\xE1s los motores
